

<!DOCTYPE html>
<html>
  <head>
    <title>Faux Flix</title>
    <%= favicon_link_tag asset_path('favicon.ico') %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.logoURL="<%= image_url("fauxflix_logo.png") %>"
      window.backgroundURL="<%= image_url("background.jpg") %>"
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
